package controle;

import java.util.Scanner;

public class DesafioDiaDaSemana {
	
	public static void main(String[] args) {
		
		Scanner entrada = new Scanner(System.in);

		String diaDaSemana = entrada.nextLine();
		int dia = 0;
		
		if("Domingo".equalsIgnoreCase(diaDaSemana)) {
			dia =1;
		}
		if("Segunda".equalsIgnoreCase(diaDaSemana)) {
			dia =2;
		}
		if("Terça".equalsIgnoreCase(diaDaSemana)) {
			dia =3;
		}
		if("Quarta".equalsIgnoreCase(diaDaSemana)) {
			dia =4;
		}
		if("Quinta".equalsIgnoreCase(diaDaSemana)) {
			dia =5;
		}
		if("Sexta".equalsIgnoreCase(diaDaSemana)) {
			dia =6;
		}
		if("Sabado".equalsIgnoreCase(diaDaSemana)) {
			dia =7;
		}
		
		else if(dia >0){
			System.out.printf("O Número Digitado é Invalido %s %d ",diaDaSemana,dia);
		}
		else {
			System.out.println("Dia Inexistênte");
			
		}
		entrada.close();
		
	}

}
