package controle;

import java.util.Scanner;

public class DesafioDiaDaSemana {
	
	public static void main(String[] args) {
		
		Scanner entrada = new Scanner(System.in);
		
		String diaDaSemana = entrada.next();
		int dia = 0;
		
		if(diaDaSemana.equalsIgnoreCase("Domingo".trim())) {
			dia=1;
		}
		else if(diaDaSemana.equalsIgnoreCase("Segunda".trim())) {
			dia=2;
		}
		
		else if(diaDaSemana.equalsIgnoreCase("Terça".trim())) {
			dia=3;
		}
		else if(diaDaSemana.equalsIgnoreCase("Quarta".trim())) {
			dia=4;
		}
		else if(diaDaSemana.equalsIgnoreCase("Quinta".trim())) {
			dia=5;
		}
		else if(diaDaSemana.equalsIgnoreCase("Sexta".trim())) {
			dia=6;
		}
		else if(diaDaSemana.equalsIgnoreCase("Sabado".trim())) {
			dia=7;
		}
		
		else {
			System.out.println("Dia Inexistênte"+dia);
		}
		entrada.close();
		
		
	}

}
