package controle;

import java.util.Scanner;

public class DesafioDiaDaSemana {
	
	public static void main(String[] args) {
		
		Scanner entrada = new Scanner(System.in);

		String diaDaSemana = entrada.nextLine();
		
		if("Domingo".equalsIgnoreCase(diaDaSemana)){
			System.out.println(1);	
		}
		if("Segunda".equalsIgnoreCase(diaDaSemana)){
			System.out.println(2);	
		}
		if("Ter√ßa".equalsIgnoreCase(diaDaSemana)){
			System.out.println(3);	
		}
		if("Quarta".equalsIgnoreCase(diaDaSemana)){
			System.out.println(4);	
		}
		if("Quinta".equalsIgnoreCase(diaDaSemana)){
			System.out.println(5);	
		}
		if("Sexta".equalsIgnoreCase(diaDaSemana)){
			System.out.println(6);	
		}
		if("Sabado".equalsIgnoreCase(diaDaSemana)){
			System.out.println(7);	
		}
		
		
		
		
		entrada.close();
		
		
	}

}
