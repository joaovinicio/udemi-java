package controle;

import java.util.Scanner;

public class DesafioWhile {
	
	public static void main(String[] args) {
		
		
		Scanner teclado = new Scanner(System.in);
		
		int quantidade = 0;
		double nota = 0;
		double total=0;
		
		while(nota !=-1) {
			System.out.println("Digite uma nota.: ");
		}
		
		if(quantidade >=1 && nota >=1) {
			
			total = quantidade / nota;
		}
		
		
		teclado.close();
		
	}

}
